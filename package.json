{
  "name": "isc-to-kea-migrator",
  "version": "1.0.1",
  "description": "Migrate ISC DHCP configurations to Kea DHCP format",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "start": "bun run src/index.ts",
    "dev": "bun --watch src/index.ts",
    "build": "bun build src/index.ts --compile --outfile isc-to-kea",
    "test": "bun test",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "dhcp",
    "isc",
    "kea",
    "migration",
    "opnsense"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "fast-xml-parser": "^4.3.4",
    "ip-num": "^1.5.1"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.9.3"
  }
}
